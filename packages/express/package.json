{
  "name": "@lensjs/express",
  "version": "1.3.3",
  "description": "Express adapter for LensJs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "express",
    "lens",
    "monitoring",
    "debugging",
    "logging",
    "database",
    "queries",
    "requests",
    "cache"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lensjs/lens.git",
    "directory": "packages/express"
  },
  "homepage": "https://lensjs.vercel.app/",
  "bugs": {
    "url": "https://github.com/lensjs/lens/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "test": "vitest run"
  },
  "dependencies": {
    "@lensjs/core": "workspace:*",
    "@lensjs/date": "workspace:*",
    "@lensjs/typescript-config": "workspace:*",
    "@lensjs/watchers": "workspace:*",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
