{
  "name": "@lensjs/express",
  "version": "1.0.7",
  "description": "Express adapter for LensJs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
