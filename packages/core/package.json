    "emittery": "^1.2.0",
    "libsql": "^0.5.17",
    "luxon": "^3.7.1",
    "rimraf": "^6.0.1",
    "sql-formatter": "^15.6.6",
    "tailwind-merge": "^3.3.1"
  },
  "gitHead": "a229fde5700970ec290ff31f1e3b7d84cc56d325"
