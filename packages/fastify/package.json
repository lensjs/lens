{
  "name": "@lensjs/fastify",
  "version": "1.0.1",
  "description": "Fastify adapter for LensJs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "test": "vitest run"
  },
  "dependencies": {
    "@fastify/static": "8.2.0",
    "@lensjs/core": "workspace:*",
    "@lensjs/date": "workspace:*",
    "@lensjs/typescript-config": "workspace:*",
    "@lensjs/watchers": "workspace:*"
  },
  "devDependencies": {
    "fastify": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "fastify": "^5.2.1"
  }
}
